@page "/force-logout"
@inherits ComponentCore<ForceLogout>

<script>
    document.cookie = 'AppServiceAuthSession=; Path=/; Expires=Thu, 01 Jan 1970 00:00:00 GMT';
</script>

@code {
    protected override void OnInitialized()
    {
        Navigation.NavigateTo("/.auth/logout", forceLoad: true);
    } 
}
