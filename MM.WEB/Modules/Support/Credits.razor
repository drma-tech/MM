@page "/support/credits"
@inherits PageCore<Credits>

<SeoHeader Title="Credits" Url=@("/support/credits")></SeoHeader>

<MudGrid>
    <MudItem xs="12" sm="6">
        <HeadPage Title="JustWatch" ImageSource="logo/justwatch.png" Link="https://www.justwatch.com/" Text="All your streaming services in one app."
                  IsAuthenticated="IsAuthenticated" ShowShare="false">
        </HeadPage>
    </MudItem>
    <MudItem xs="12" sm="6">
        <HeadPage Title="MudBlazor" ImageSource="logo/mudblazor.png" Link="https://mudblazor.com/"
                  Text="Blazor Component Library based on Material Design. MudBlazor is easy to use and extend, especially for .NET devs because it uses almost no Javascript."
                  IsAuthenticated="IsAuthenticated" ShowShare="false">
        </HeadPage>
    </MudItem>
</MudGrid>
